{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@core/*": ["../src/core/*"],
      "@assets/*": ["./assets/*"]
    }
  },
  "include": ["src/**/*", "../src/core/**/*"]
}
