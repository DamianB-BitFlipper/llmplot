{
  "name": "llmplot",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "llmplot": "./src/index.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "generate:assets": "bun run scripts/generate-assets.ts",
    "generate:assets:watch": "bun run scripts/generate-assets.ts --watch",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare": "husky",
    "website:dev": "astro dev --root src/website",
    "website:build": "astro build --root src/website",
    "website:preview": "astro preview --root src/website"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "astro": "^5.16.6",
    "autoprefixer": "^10.4.23",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.562.0",
    "puppeteer": "^24.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.5",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.33.1"
  }
}
